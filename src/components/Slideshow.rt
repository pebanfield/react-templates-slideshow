<div class="w3-content w3-display-container">
    <button class="w3-btn-floating w3-display-left"
       onClick="()=>this.showPrevSlide()">&#10094;</button>
    <button class="w3-btn-floating w3-display-right"
       onClick="()=>this.showNextSlide(+1)">&#10095;</button>
    <img class="slides"
         rt-repeat="image in this.state.images"
         key="{image.id}"
         src="{image.src}">
</div>